{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react",
  ],
  "plugins": [
    ["import", { // antd-mobile 按需加载组件
      "libraryName": "antd-mobile",
      "libraryDirectory": "es",
      "style": true,
    }],
    ["@babel/plugin-proposal-class-properties"],
    ["@babel/plugin-syntax-dynamic-import"],
    ["@babel/plugin-transform-runtime"]
  ]
}
